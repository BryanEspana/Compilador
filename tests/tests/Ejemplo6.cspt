class CajaRegistradora {
  let impuesto: integer;
  let total: integer;
  
  function constructor(impuesto: integer) {
    this.impuesto = impuesto;
    this.total = 0;
  }

  function agregarProducto(precio: integer): void {
    this.total = this.total + precio;
    print("Producto anadido con precio: " + precio);
  }

  function calcularTotalConImpuesto(): integer {
    let totalConImpuesto: integer = this.total + (this.total * this.impuesto / 100);
    print("El total con impuesto es: " + totalConImpuesto);
    return totalConImpuesto;
  }

  function reiniciar(): void {
    this.total = 0;
    print("La caja registradora se ha reiniciado.");
  }
}

// Uso de la caja registradora
let caja: CajaRegistradora = new CajaRegistradora(12);
caja.agregarProducto(100);
caja.agregarProducto(50);
caja.agregarProducto(25);
caja.calcularTotalConImpuesto();
caja.reiniciar();
